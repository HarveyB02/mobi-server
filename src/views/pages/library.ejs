<!DOCTYPE html>
<head>
	<%- include('../partials/head') %>
</head>

<body>
	<header>
		<%- include('../partials/header') %> 
	</header>

	<main>
		<div class="row">
			<% for (let x = 0; x < splitData.length; x++) { %>
				<div style="width: 33.33%;">
					<% for (let y = 0; y < splitData[x].length; y++) { %>
						<input type="image" src="<%= splitData[x][y].coverUrl %>" onclick="window.location = '/manga/<%= encodeURIComponent(splitData[x][y].mangaUrl) %>'" style="width: 100%">
					<% } %>
				</div>
			<% } %>
		</div>
	</main>
</body>